{
  "name": "maas-schemas-ts",
  "version": "0.0.1",
  "description": "TypeScript types and io-ts validators for maas-schemas",
  "main": "index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "eslint": "eslint --max-warnings=0 '**/*.{ts,json}' --ignore-path .gitignore",
    "lint": "yarn eslint",
    "clean": "rm -rf lib",
    "build": "yarn clean && tsc",
    "ci": "yarn lint && yarn build",
    "convert": "./converter.ts",
    "convert-all": "(cd node_modules/maas-schemas/schemas/; find . -iname '*.json' -exec ../../../converter.ts {} ../../../src \\;) && yarn eslint --fix",
    "deploy-npm": "yarn ci && yarn publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maasglobal/maas-schemas-ts.git"
  },
  "keywords": [
    "schemas",
    "maas",
    "io-ts",
    "TypeScript"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maasglobal/maas-schemas-ts/issues"
  },
  "homepage": "https://github.com/maasglobal/maas-schemas-ts#readme",
  "peerDependencies": {
    "io-ts": "^2.0.1"
  },
  "devDependencies": {
    "@types/json-schema": "^7.0.3",
    "@types/node": "^12.7.5",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-prettier": "^3.1.0",
    "fp-ts": "^2.0.5",
    "io-ts": "^2.0.1",
    "io-ts-codegen": "^0.4.1",
    "maas-schemas": "maasglobal/maas-schemas",
    "prettier": "^1.18.2",
    "typescript": "^3.5.3"
  }
}
